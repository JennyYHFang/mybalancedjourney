<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Date</th>
      <th>Weight</th>
      <th>Exercise</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% dailylogs.forEach(function(m) { %>
      <tr>
        <td><%= m.date %></td>
        <td><%= m.weight %></td>
        <td><%= m.exercise %></td>
        <td><a href="/dailylogs/<%= m._id %>">details</a></td>
        <td><form action="/dailylogs/<%= m._id %>?_method=DELETE" method="POST">
            <input type="submit" value="X">
        </form></td>
      </tr>
      <% }); %>
    </tbody>
  </table>


  
<%- include('../partials/footer') %>
