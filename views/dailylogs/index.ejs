<%- include('../partials/header') %>

<table id="tablelist">
  <thead>
    <tr>
      <th>Date</th>
      <th>Nickname</th>
      <th>Weight</th>
      <th>Exercise</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% dailylogs.forEach(function(m) { %>
      <tr>
        <td><%= m.date %></td>
        <td><%= m.name %></td>
        <td><%= m.weight %></td>
        <td><%= m.exercise %></td>
        <td><a href="/dailylogs/<%= m._id %>">details</a></td>
        <td><form action="/dailylogs/<%= m._id %>?_method=DELETE" method="POST">
            <input type="submit" value="X">
        </form></td>
      </tr>
      <% }); %>
    </tbody>
  </table>

  <img class="exerciselog" src="https://static01.nyt.com/images/2016/04/26/health/well_guides_listy_assets/well_guides_listy_assets-superJumbo.gif" alt="exercise">


  
<%- include('../partials/footer') %>
